<tr class="assessments-bulk-complete-table__row">
  <th class="assessments-bulk-complete-table__column_sticky">
    <div class="assessments-bulk-complete-table__cell">
      Title
    </div>
    <div class="assessments-bulk-complete-table__cell">
      Status
    </div>
  </th>
  {{#headersData}}
    <th class="assessments-bulk-complete-table__cell">
      {{#if mandatory}}
        <i class="fa fa-asterisk field__mandatory"></i>
      {{/if}}
      {{title}}
    </th>
  {{/headersData}}
</tr>
