<tr>
  <th>
    <table>
      <tr>
        <td>
          <a href="/assessments/{{rowData.asmtId}}" target="_blank">
            {{rowData.asmtTitle}}
          </a>
        </td>
        <td>
          <span class="state-value-dot {{addclass 'state' rowData.asmtStatus}}">
            {{rowData.asmtStatus}}
          </span>
        </td>
      </tr>
    </table>
  </th>
  {{#each rowData.attributes}}
    <td>
      {{#if isApplicable}}
        {{value}}
      {{else}}
        <span class="empty-message">Not applicable</span>
      {{/if}}
    </td>
  {{/each}}
</tr>
